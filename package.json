{
  "name": "n8n-nodes-greyhound",
  "version": "1.5.0",
  "description": "n8n community node for GREYHOUND DMS REST API - Access colors, groups and items endpoints with filtering, pagination and creation",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/GreyhoundApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Greyhound/Greyhound.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "greyhound",
    "rest",
    "api",
    "dms",
    "document-management"
  ],
  "author": "Michael Gross <info@michaelgross.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Micha0827/n8n-nodes-greyhound.git"
  },
  "homepage": "https://github.com/Micha0827/n8n-nodes-greyhound#readme",
  "bugs": {
    "url": "https://github.com/Micha0827/n8n-nodes-greyhound/issues"
  }
}
